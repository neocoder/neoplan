{
    "name": "neoplan",
    "version": "1.3.4",
    "description": "Lightweight MongoDB based Node.js job scheduler",
    "main": "dist/neoplan.js",
    "types": "dist/neoplan.d.ts",
    "scripts": {
        "test": "mocha",
        "test-in-docker": "docker-compose up --abort-on-container-exit",
        "test-in-docker-wait": "docker-compose up",
        "build": "rm -rf dist && tsc",
        "prepublishOnly": "rm -rf dist && tsc",
        "build_n_test": "npm run build && npm test"
    },
    "keywords": [
        "job",
        "jobs",
        "scheduler",
        "cron",
        "task"
    ],
    "author": "Alex Ladyga",
    "license": "MIT",
    "dependencies": {
        "@types/human-interval": "^1.0.0",
        "async": "^3.2.3",
        "debug": "^2.2.0",
        "human-interval": "^0.1.6",
        "mongodb": "^4.1.3",
        "mongoose": "^6.5.2",
        "typescript": "^4.7.4"
    },
    "devDependencies": {
        "@types/async": "^3.2.15",
        "@types/debug": "^4.1.7",
        "chai": "^4.3.4",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "mocha": "^9.1.2",
        "prettier": "^2.6.2"
    }
